<page id="main">
    <agent
      id="key"
      image="key.png"
       
      x="400"
      y="300"
      fab-if="score &gt;= 10"
      on_click="has_key = True"
      
      />

    <agent
      id="door"
      image="door_closed.png"
      x="500"
      y="200"
      fab-if="not has_key"/>
    <agent
      id="door"
      image="door_open.png"
      x="500"
      y="200"
      fab-if="has_key"/>
</page>
